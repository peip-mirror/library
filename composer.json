{
    "name": "swoole/library",
    "description": "The swoole library.",
    "keywords": ["swoole", "library"],
    "license": "apache-2.0",
    "homepage": "https://www.swoole.com",
    "support": {
        "issues": "https://github.com/swoole/library/issues",
        "source": "https://github.com/swoole/library"
    },
    "authors": [
        {
            "name": "Rango",
            "email": "rango@swoole.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "ext-swoole": "*"
    },
    "suggest" : {
        "amphp/amp": "^2.0.3",
        "kmelia/monolog-stdout-handler": "^1.2",
        "nmred/kafka-php": "dev-master",
        "react/event-loop": "dev-master",
        "monolog/monolog": "^1.23"
    },
    "autoload": {
        "files": [
            "src/Swoole/Foundation/helpers.php"
        ],
        "psr-4": {
            "Swoole\\": "src/Swoole/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
